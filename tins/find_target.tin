		#class find_target_baihuagu_inner kill;
		#class find_target_zizhulin_inner kill;
		#class find_target_manglin_inner kill;
		follow $target_name_lower;
	};
	#action {你要对谁做这个动作} {
		#math temppl {$temppl + 1};
		#if {$temppl == 1} {climb left;wink $target_name_lower};
		#if {$temppl == 2 || $temppl == 3} {climb up;wink $target_name_lower};
		#if {$temppl == 4 || $temppl == 5} {climb left;wink $target_name_lower};
		#if {$temppl == 6 || $temppl == 7} {climb up;wink $target_name_lower};
		#if {$temppl == 8} {idle;wink $target_name_lower};
		#if {$temppl > 8} {tkill temp;#delay 2 {findxs6};};
	};
	#class temp close;
	climb up;wink $target_name_lower;
}
