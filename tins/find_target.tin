#alias findxs8 {
	tkill temp;
	#var tempxs {1};
	#class temp open;
	#action	{你用迷人的眼眸对} {
		#class temp kill;
		#class find_target_baihuagu_inner kill;
		#class find_target_zizhulin_inner kill;
		#class find_target_manglin_inner kill;
		follow $target_name_lower;
	};
	#action {你要对谁做这个动作} {
		#math tempxs {#tempxs + 1};
		#delay 1 {
			#switch {tempxs} {
				#case 1 {n};
				#case 2 {w};
				#case 3 {w};
				#case 4 {n};
				#case 5 {w};
				#case 6 {w};
				#case 7 {n};
				#case 8 {w};
				#case 9 {w};
				#case 10 {tkill temp;findnext};
			}; 
			wink $target_name_lower;
		};
	};
	#class temp close;
	n;wink $target_name_lower;
}

