#read tins/pathdir.tin
#var target_spotified 0;
#var target_found 0;
#var target_followed 0;
#var body_guard_ready 0;
#nop var correct_target_found 0;
#nop var target_name_lower {};
#nop format {target_name_upper} {%n} {$target_name_lower};
#nop var target_name_chinese {};

#var target_found 0;
#nop var target_name_lower hand
#nop var target_name_upper Hand
#nop var target_name_chinese 寒笛

#nop alias {go_find_target_with_known_location %1 in %2} {
	#var target_name_lower %1;
	#format {target_name_upper} {%n} {$target_name_lower};
	#var target_location %2;
	turn_on_search_target_inner_class $target_name_lower in $target_location;
}

#action {^%?%?%?千里眼告诉你：奇怪，找不到这个人．．．} {
	#showme {<fff>TARGET $target_name_lower DISAPPEARED};
	tkill find_target;
}
#highlight {$target_name_chinese($target_name_upper)} {white};



#action {^千里眼告诉你：%1($target_name_lower)在%2。} {
	#nop #var target_name_chinese %1;
	#format {target_name_upper} {%n} {$target_name_lower};
	#var target_location %2;
	
	#nop if {&{body_guard}} ;
	#nop {tell $body_guard TASK:gofindnpcwithknownlocation $target_name_lower, in $target_location;};
	#nop delay 2 {turn_on_search_target_inner_class $target_name_chinese $target_name_lower $target_location};
	#nop delay 2 {turn_on_search_target_inner_class};
	#echo {<fff>开始找怪};
	prepare_guai_locations;
}

#alias fguai {
	#nop var target_name_chinese {%1};
	#nop var target_name_lower {%2};
	#nop var target_location {%3};
	#nop format {target_name_upper} {%n} {$target_name_lower};
	#nop var GLpath {$target_location};
	#var GLslowrun 0;
	#math mfg {$mfg + 1};
	#var FGcount 0;
	#var FGfound 0;
	#var FGsum 0;
	
	turn_on_search_target_inner_class;
	
	#if {$GLslowrun == 1} {
		#nop #path load $GLpath;
		#nop #foreach $GLpath;
	}; #else {
		#path load $GLpath;
		#path run;
		set brief path_check;
		#echo {<fff>开始执行$GLpath路径}；
	};	
};

#alias turn_on_search_target_inner_class {
	#class find_target_search_inner open;
	
	#action	{$target_name_chinese($target_name_upper)} {
		#if {$FGfound == 1} {
			#var FGcount 0;
		}; #else {
			#var FGfound 1;
		};
		#echo {<fff>看到目标啦！！};
	};
	
	#action	{{这里唯一的出口是|这里明显的出口是|你将%*打开。|什么？|%*已经是开着的了。|这个方向没有出路|这里没有任何明显的出|乌鸦先生说道：请吧}} {
		#math FGsum {$FGsum + 1};
		#if {$FGfound == 1} {
			#nop {FGcound means that after you found guai, how many steps you have moved away};
			#math FGcount {$FGcount + 1};
		}; #else {
			#var FGcount 0;
		};
	};
	
	#action	{设定环境变数%*brief%*path_check} {
		#line {gag};
		#nop var mtime=1;
		#nop math mfg2 {$mfg2 + 1};
		#delay 1 {
			#if {$FGfound == 0} {
				$GLexecNO;
			}; #else {
				#nop FGcount=[$FGcount];
				#if {$FGcount > 0} {
					#path load $GLpath;
					#LOOP 1 {$FGcount+1} cnt {#path walk backwards};
					#var FGfound 0;
				}; 
				
				attempt_to_follow_target;
				#var FGcount 0;
				#nop #T+ Fight1;
				#nop #T+ fight3;
				#nop ResetTrigger2;
				#nop #T+ backup;
			};
		};
		tkill find_target_search_inner;
	};
	
	#action	{看起来$target_name_chinese想杀死你} {
		#if {$FGfound == 1} {
			#var FGcount {0};
		}; #else {
			#var FGfound {1};
		};
		#if { $GLslowrun == 1} {
			#nop path end;
			#nop fguaia;
		};
	};
	
	#action	{你手捻口诀，念动真言，摇身一变，变得和%*一模一样！} {#var bian {1}};
	#class find_target_search_inner close;
};

#alias attempt_to_follow_target {
	#class find_target_follow_inner open;
	#var follow_attempts {0};
	#action {^这里没有%s$target_name_lower。} {
		#math follow_attempts {$follow_attempts + 1};
		#if {$follow_attempts > 10} {
			#class find_target_follow_inner kill;
			#class find_target kill;
			#showme {<fff>FAILED TO FIND TARGET $target_name_lower};
		}; #else {
			#delay 1 {
				random;
				follow $target_name_lower;
			};
		};
	};

	#action {^%?%?%?%?你决定开始跟随$target_name_chinese一起行动。} {
		#var target_found 1;
		#var target_followed 1;
		#showme {<fff>SUCCESSFULLY FOUND TARGET $target_name_lower};
		#nop class paths kill;
		#class find_target_follow_inner kill;
		#class find_target kill;
	};

	#class find_target_follow_inner close;
	follow $target_name_lower;
}

